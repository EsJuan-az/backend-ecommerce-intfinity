    "url": "https://github.com/EsJuan-az/backend-ecommerce-infinity/issues"
  },
  "homepage": "https://github.com/EsJuan-az/backend-ecommerce-infinity#readme",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "shortid": "^2.2.16",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2"
  }
