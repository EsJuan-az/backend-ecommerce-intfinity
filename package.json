{
  "name": "backend-intfinity",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migration:generate": "sequelize-cli migration:generate --name",
    "migration:run": "sequelize-cli db:migrate",
    "migration:revert": "sequelize-cli db:migrate:undo",
    "migration:reset": "sequelize-cli db:migrate:undo:all",
    "migration:debug": "sequelize-cli db:migrate --debug",
    "migration:file": "npx sequelize-cli db:migrate --to",
    "build": "npm install && npm run migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EsJuan-az/backend-ecommerce-infinity.git"
  },
